/* 授权模块 */
.auth-win-wrap{
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 1000;
}
.auth-mask{
  top: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.34);
}
.auth-win{
   width: 200px;
  height: 210px;
  position: absolute;
  left: 50%;
  margin-left: -100px;
  top: 50%;
  margin-top: -115px;
  background: #ffffff;
  border-radius: 10px;
}
.auth-close{
  width: 25px;
  height: 25px;
  position: absolute;
  top: 3px;
  right: 5px;
}
.auth-close-left{
 width:2px;
  height:14px;
  background:#9c9b9b;
  transform:rotate(45deg);
  position:absolute;
  left:10px;
  top:5px;
}
.auth-close-right{
  width:2px;
  height:14px;
  background:#9c9b9b;
  transform:rotate(-45deg);
  position:absolute;
  left:10px;
  top:5px;
}
.auth-title{
  font-size: 18px;
  line-height: 30px;
  height: 30px;
  border-bottom: 1rpx solid #dcdcdc;
  text-align: center;
}
.auth-win-desc{
  font-size: 14px;
  margin-top: 48px;
  text-align: center;
}
.auth-button{
  position: absolute;
  width: 150px;
  bottom: 20px;
  left: 25px;
  border-radius: 5px;
  background-color: #ff5353;
}
.userinfo-avatar{
  height: 40px;
  width: 100%;
  line-height: 40px;
  background-color: #ff5353;
}
