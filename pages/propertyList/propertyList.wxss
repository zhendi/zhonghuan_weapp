  padding-right: 17px;
}
.more-adviser-title{
  font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #231815;
}
.more-adviser-icon{
  width: 6px;
	height: 12px;
  margin-left: 8px;
}
.adviser-desc{
  font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #231815;
}
.adviser-level{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 65px;
  height: 20px;
  border-top-right-radius: 8px;
}
.bottom-desc-wrap{
  margin-top: 10px;
}
.bottom-desc{
  font-size: 10px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #9fa0a0;
  line-height: 29px;
  text-align: center;
}
/* 资讯 */
.info-container{
  margin-top: 30px;
}
.info-top{
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 17px;
}
.info-top-title{
  font-size: 25px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #000000;
}
.info-more-wrap{
  display: flex;
  align-items: center;
}
.adress-arrow-right{
  width: 9px;
  height: 12px;
  margin-left: 13px;
}
.info-more{
  font-size:12px;
  font-weight:normal;
  font-stretch:normal;
  letter-spacing:0px;
 color: #595757;
}
.info-content{
	height: 214px;
  width: 100%;
  white-space: nowrap;
}
.info-item{
  width: 300px;
  height:160px;
  padding: 16px 13px;
  background-color: #ffffff;
  box-shadow: 3px 7px 24px 0px 
		rgba(25, 25, 25, 0.24);
  border-radius: 8px;
  display: inline-block;
  margin-right:12px;

}
.info-item:first-child{
  margin-top:10px;
  margin-left:17px;
}
.info-title-wrap{
  font-size:17px;
  height: 47px;
  width: 85%;
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  word-break: break-all;
  flex-wrap: wrap;
  color: #231815;
  white-space: pre-wrap;
}
.info-middle{
  display: flex;
  flex-direction: row;
  margin-top: 13px;
}
.info-image{
  width: 100px;
  height: 60px;
  background-color: #c8c8c8;
  align-items: center;
  border-radius: 4px;
}
.info-desc{
  margin-left:10px;
  margin-right: 20px;
  font-size: 14px;
  height: 35px;
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  color: #595757;
  margin-top:4px;
  white-space: pre-wrap;
}
.info-view{
  color: #727171;
  font-size: 12px;
  margin-top: 10px;
}



