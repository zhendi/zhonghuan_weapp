/* 授权模块 */
.auth-win-wrap{
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 1000;
}
.auth-mask{
  top: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.34);
}
.auth-win{
   width: 200px;
  height: 210px;
  position: absolute;
  left: 50%;
  margin-left: -100px;
  top: 50%;
  margin-top: -115px;
  background: #ffffff;
  border-radius: 10px;
}
.auth-close{
  width: 25px;
  height: 25px;
  position: absolute;
  top: 3px;
  right: 5px;
}
.auth-close-left{
 width:2px;
  height:14px;
  background:#9c9b9b;
  transform:rotate(45deg);
  position:absolute;
  left:10px;
  top:5px;
}
.auth-close-right{
  width:2px;
  height:14px;
  background:#9c9b9b;
  transform:rotate(-45deg);
  position:absolute;
  left:10px;
  top:5px;
}
.auth-title{
  font-size: 18px;
  line-height: 30px;
  height: 30px;
  border-bottom: 1rpx solid #dcdcdc;
  text-align: center;
}
.auth-win-desc{
  font-size: 14px;
  margin-top: 48px;
  text-align: center;
}
.auth-button{
  position: absolute;
  width: 150px;
  bottom: 20px;
  left: 25px;
  border-radius: 5px;
  background-color: #ff5353;
}
.userinfo-avatar{
  height: 40px;
  width: 100%;
  line-height: 40px;
  background-color: #ff5353;
}
/* 授权模块 */
.padding{
  padding-left: 17px;
  padding-right: 17px;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.position{
  margin-top: 22px;
  display: flex;
  align-items: center;
}
.city{
  font-size: 22px;
	font-weight: normal;
	font-stretch: normal;
	color: #000000;
}
.position-icon{
  width: 20px;
  height: 21px;
}
.pictures-list{
  width: 100%;
  height: 190px;
  margin-top: 17px;
}
.pictures-list-item{
  padding-left: 17px;
  padding-right: 17px;
  height: 100%;
}
.pictures-list-item-image{
  width: 100%;
  height: 100%;
  border-radius: 10px;
  background-color: #bbb;
}
.propertys-container{
  margin-top: 30px;
}
.homes-desc{
  font-size: 25px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #000000;
}
.line{
  margin-left: 17px;
  margin-right: 17px;
  box-sizing: border-box;
	height: 1rpx;
	background-color: #dcdcdc;
  margin-top: 11px;
}
.name-position{
  margin-top: 20px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.name-position .name{
  font-size: 22px;
  line-height: 22px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #000000;
}
.name-position .area{
font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #9fa0a0;
}
.detail-position-wrap{
  margin-top: 16px;
}
.detail-position{
  font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #595757;
}
.detail-info{
  margin-top:25px;
  display: flex;
  flex-direction: row;
}
.property-image-wrap{
  width: 195px;
	height: 135px;
	background-color: #bbbbbb;
	border-radius: 7px;
  overflow: hidden;
}
.property-image{
  width: 100%;
  height: 100%;
}
.other-info{
  margin-left: 11px;
  overflow: hidden;
  flex: 1;
}
.no-price{
  font-size: 22px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #9fa0a0;
}
.no-price-wrap{
  margin-bottom: 10px;
  margin-top: -2px;
}
.price-wrap{
  display: flex;
  flex-direction: column;
}
.price{
  font-size: 18px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #e70012;
}
.price-desc{
  margin-top: 2px;
  font-size: 12px;
  color: #898989;
}
watch-people{
  font-family: PingFang-SC-Bold;
  font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #000000;
}
.lable-wrap{
  margin-top: -2px;
}
.property-lable{
  display: inline-block;
  border-radius: 5px;
	border: solid 1rpx #a0a0a0;
  padding: 0px 3px;
  font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #898989;
  margin-right: 4px;
  margin-top: 0px;
}
.adviser-info{
  margin-top: 22px;
  margin-left: 17px;
}
.adviser-container{
	height: 101px;
  width: 100%;
  white-space: nowrap;
}
.adviser-wrap{
  width: 130px;
	height: 101px;
  display: inline-block;
  margin-right: 7px;
  position: relative;
}
.adviser{
  width: 100%;
	height: 100%;
	background-color: #bbbbbb;
	border-radius: 7px;
}
.adviser-desc-wrap{
  margin-top: 13px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.adviser-desc-left{
  font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
